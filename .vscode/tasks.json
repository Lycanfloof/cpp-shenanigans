{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "G++ 11.3.0",
			"command": "/usr/bin/g++",
			"args": [
				"-g",
				"-std=c++23",
				"-fmodules-ts",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${fileDirname}/main"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /usr/bin/g++"
		},
		{
			"type": "cppbuild",
			"label": "G++ 13.1.0",
			"command": "/home/linuxbrew/.linuxbrew/bin/g++-13",
			"args": [
				"-g",
				"-std=c++23",
				"-fmodules-ts",
				"${workspaceFolder}/*.cpp",
				"-o",
				"${fileDirname}/main"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: /home/linuxbrew/.linuxbrew/bin/g++-13"
		}
	]
}